{
  "expo": {
    "name": "PDI System",
    "displayName": "PDI System",
    "slug": "pdi-checklist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fs-mini-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/fs-mini-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fs-mini-logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "android.permission.CAMERA"
      ],
      "package": "com.anonymous.pdichecklist"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f9d77804-70c1-4212-94dd-3bd571b812ef"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}
